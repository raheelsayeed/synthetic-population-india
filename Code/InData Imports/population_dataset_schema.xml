<database>
    <table name="Region">
        <column name="row_id"               type="int" primary="1"/>
        <column name="code"                 type="int" />
        <column name="name"                 type="text" />
        <column name="level"                type="int" />
        <column name="type"                 type="int" />
        <column name="statecode"            type="int" default="0" />
        <column name="districtcode"         type="int" default="0" />
        <column name="subdistrictcode"      type="int" default="0" />
        <column name="tvcode"               type="int" default="0" />
        <column name="area"                 type="int" />
    </table>

    <table name="Household">
        <column name="row_id" type="int" primary="1" />
        <column name="region_code" type="int"/>
        <column name="total"    type="int"/>
        <column name="size_1"   type="int"/>
        <column name="size_2"   type="int"/>
        <column name="size_3"   type="int"/>
        <column name="size_4"   type="int"/>
        <column name="size_5"   type="int"/>
        <column name="size_6"   type="int"/>
        <column name="size_7_10"   type="int"/>
        <column name="size_11_15"   type="int"/>
        <column name="size_15_plus"   type="int"/>
        <column name="size_mean"   type="varchar(36)"/>
    </table>

    <table name="Population">
        <column name="row_id"           type="int" primary="1"/>
        <column name="region_code"      type="int" />
        <column name="total"            type="int" />
        <column name="males"        type="int" />
        <column name="females"      type="int" />
        <column name="A0_6_T"       type="int" />
        <column name="A0_6_M"       type="int" />
        <column name="A0_6_F"       type="int" />
        <column name="A0_4"         type="int" />
        <column name="A5_9"         type="int"/>
        <column name="A10_14"         type="int"/>
        <column name="A15_19"         type="int"/>
        <column name="A20_24"         type="int"/>
        <column name="A25_29"         type="int"/>
        <column name="A30_34"         type="int"/>
        <column name="A35_39"         type="int"/>
        <column name="A40_44"         type="int"/>
        <column name="A45_49"         type="int"/>
        <column name="A50_54"         type="int"/>
        <column name="A55_59"         type="int"/>
        <column name="A60_64"         type="int"/>
        <column name="A65_69"         type="int"/>
        <column name="A70_74"         type="int"/>
        <column name="A75_79"         type="int"/>
        <column name="A80"            type="int"/>
    </table>
    
    <table name="Workers">
        <column name="id"           type="int" primary="1"/>
        <column name="region_code"  type="int" />
        <column name="total"        type="int" />
        <column name="males"        type="int" />
        <column name="females"      type="int" />
    </table>
    
    <table name="Schools">
        <column name="id"              type="int" primary="1"/>
        <column name="region_code"     type="int" />
        <column name="total"        type="int" />
        <column name="primary"      type="int" />
        <column name="middle"       type="int" />
        <column name="secondary"    type="int" />
        <column name="colleges"     type="int" />
    </table>
    
    
</database> 